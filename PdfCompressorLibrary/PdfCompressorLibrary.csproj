<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>net45;net4.8</TargetFrameworks>
    <AssemblyTitle>PdfCompressorLibrary</AssemblyTitle>
    <Company>EPAM Systems</Company>
    <Product>PdfCompressorLibrary</Product>
    <Copyright>Copyright © EPAM Systems 2017</Copyright>
    <OutputPath>bin\$(Configuration)\</OutputPath>
    <PackageId>$(AssemblyName)</PackageId>
    <IncludeSymbols>True</IncludeSymbols>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="iTextSharp" Version="5.5.11" />
    <PackageReference Include="VVVV.FreeImage" Version="3.15.1.1" />
  </ItemGroup>
  <ItemGroup>
    <Reference Include="System.configuration" />
    <Reference Include="System.Data.DataSetExtensions" />
    <Reference Include="Microsoft.CSharp" />
    <Reference Include="System.Net.Http" />
  </ItemGroup>
</Project>